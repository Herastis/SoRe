@prefix : <http://www.sore.org/person#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Classes
:Person rdf:type rdfs:Class .

# Properties
:alumniOf rdf:type rdf:Property ;
          rdfs:domain :Person ;
          rdfs:range xsd:string ;
          rdfs:comment "The educational institution from which the person graduated." .

:workLocation rdf:type rdf:Property ;
              rdfs:domain :Person ;
              rdfs:range xsd:string ;
              rdfs:comment "The location where the person works." .

:countryName rdf:type rdf:Property ;
             rdfs:domain :Person ;
             rdfs:range xsd:string ;
             rdfs:comment "The name of the country where the person is located." .

:email rdf:type rdf:Property ;
       rdfs:domain :Person ;
       rdfs:range xsd:string ;
       rdfs:comment "The email address of the person." .

:familyName rdf:type rdf:Property ;
            rdfs:domain :Person ;
            rdfs:range xsd:string ;
            rdfs:comment "The family name of the person." .

:locality rdf:type rdf:Property ;
          rdfs:domain :Person ;
          rdfs:range xsd:string ;
          rdfs:comment "The locality or city where the person is located." .

:gender rdf:type rdf:Property ;
        rdfs:domain :Person ;
        rdfs:range xsd:string ;
        rdfs:comment "The gender of the person." .

:interest rdf:type rdf:Property ;
          rdfs:domain :Person ;
          rdfs:range rdf:List ;
          rdfs:comment "A list of interests associated with the person." .

:name rdf:type rdf:Property ;
      rdfs:domain :Person ;
      rdfs:range xsd:string ;
      rdfs:comment "The name of the person." .

:status rdf:type rdf:Property ;
        rdfs:domain :Person ;
        rdfs:range xsd:string ;
        rdfs:comment "The relationship status of the person." .

:title rdf:type rdf:Property ;
       rdfs:domain :Person ;
       rdfs:range xsd:string ;
       rdfs:comment "The title or profession of the person." .